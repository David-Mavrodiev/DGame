@model List<Tuple<string, string>>

@if (Model.Count > 0)
{
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mb30">
        <div class="tutor-block">
            <div class="tutor-content">
                <div class="adds">
                    @foreach (var pair in Model)
                    {
                        <a href="@pair.Item2">
                            <img src="/Advertise/GetAdvertImage?filename=@pair.Item1" width="100" />
                        </a>
                    }
                </div>
            </div>
        </div>
        <div class="social-media">
        </div>
    </div>
}
